set xrange [3:50]
set yrange [0:1.1]
set terminal pngcairo enhanced color lw 2 size 800,600 font 'Arial-Bold'
set output "T1-Probability.png"
set xlabel "Time (sec)"
set ylabel "Drop Probability (p)"
set key outside;
set key right top;
plot "T1_1.plotme" title "T1" with lines lw 1.5

